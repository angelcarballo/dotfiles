{
  "title": "Use Cmd+G to search open tabs in Firefox",
  "rules": [
    {
      "description": "Use Cmd+G to search open tabs in Firefox",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": [
              "left_command"
            ]
          },
          "to": [
            {
              "key_code": "l",
              "modifiers": [
                "left_command"
              ]
            },
            {
              "key_code": "%"
            },
            {
              "key_code": "Spacebar"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org.mozilla.firefox"
              ]
            }
          ]
        }
      ]
    }
  ]
}
